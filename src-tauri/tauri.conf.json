{
  "build": {
    "beforeDevCommand": "cd nt && yarn dev",
    "beforeBuildCommand": "cd nt && yarn build",
    "devPath": "http://localhost:5314",
    "distDir": "../nt/.output/public",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "tauri-test",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.kjxbyz.tauri-test",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "tauri-test",
        "width": 800,
        "height": 600
      }
    ]
  }
}
